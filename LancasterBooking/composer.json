{
    "name": "lancaster/booking-system",
    "description": "Lancaster's Restaurant Booking System MVC Project",
    "type": "project",
    "require": {
      "php": ">=8.0",
      "vlucas/phpdotenv": "^5.5"
    },
    "autoload": {
      "psr-4": {
        "Core\\": "apps/core/",
        "Controllers\\": "apps/controllers/",
        "Models\\": "apps/models/"
      }
    },
    "autoload-dev": {
      "psr-4": {
        "Tests\\": "tests/"
      }
    },
    "scripts": {
      "start": "php -S localhost:8000 -t public",
      "test": "phpunit --configuration phpunit.xml"
    },
    "minimum-stability": "stable",
    "license": "MIT"
  }
